{
  "name": "starchart",
  "version": "1.0.0",
  "description": "StarWars character index using the SWAPI RestAPI",
  "main": "server.ts",
  "author": "Armin Nel",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.37",
    "apollo-server-express": "^2.22.1",
    "axios": "^0.21.1",
    "class-validator": "^0.13.1",
    "concurrently": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "react-paginate": "^7.1.2",
    "reflect-metadata": "^0.1.13",
    "swapi-node": "^0.6.0",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "start": "ts-node src/server.ts",
    "server": "nodemon --exec ts-node src/server.ts",
    "client": "yarn --cwd ./client start",
    "dev": "concurrently \"yarn run server\" \"yarn run client\""
